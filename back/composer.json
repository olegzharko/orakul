{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "bissolli/nova-phone-field": "^0.0.4",
        "classic-o/nova-media-library": "^1.0",
        "cloudcake/nova-fixed-bars": "^1.1",
        "coderello/laravel-nova-lang": "^1.7",
        "ebess/advanced-nova-media-library": "^3.4",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "genealabs/nova-prepopulate-searchable": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^8.12",
        "laravel/nova": "~3.0",
        "laravel/passport": "^10.1",
        "laravel/tinker": "^2.5",
        "naif/toggle": "^1.2",
        "optimistdigital/nova-sortable": "^2.0",
        "phpoffice/phpspreadsheet": "^1.16",
        "phpoffice/phpword": "^0.17.0",
        "simplesquid/nova-advanced-number-field": "^2.0",
        "sloveniangooner/searchable-select": "^1.3",
        "spatie/laravel-medialibrary": "^8.0.0",
        "spatie/laravel-query-builder": "^3.3",
        "spatie/laravel-translatable": "^4.6",
        "techouse/intl-date-time": "^1.6",
        "timothyasp/nova-color-field": "^1.0",
        "titasgailius/search-relations": "^1.0",
        "vyuldashev/nova-money-field": "^1.3"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ],
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
